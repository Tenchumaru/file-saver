<Application x:Class="FileSaver.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <Image x:Key="FileIcon" Source="Icons/File.ico" />
        <Image x:Key="FolderIcon" Source="Icons/Folder.ico" />
    </Application.Resources>
</Application>
